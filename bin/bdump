#!/bin/sh
# @(#) Byte dump with ASCII character. Version 1.1.0.20140708

usage() {
    echo "usage: `basename "$0"` [-h] [file ...]" 1>&2
    exit $1
}

opt=
while getopts 'h' opt; do
    case $opt in
    h)      usage 0 ;;
    \?)     usage 1 ;;
    esac
done
shift `expr $OPTIND - 1`

exec od -tx1c ${@+"$@"}
